{% extends 'base.html.twig' %}

{% block body %}
    <h1>Nom de la sortie : {{ sortie.nom }}</h1>
    <h1>{{ sortie.campus.nom }}</h1>
    <h1>{{ sortie.lieu.nom }}</h1>
    <h1>{{ sortie.organiseePar.nom }}</h1>
    <h1>Liste des participants</h1>
    {% for participants in sortie.participants %}
        <h1>{{ participants.nom }}</h1>
    {% endfor %}
{% endblock %}